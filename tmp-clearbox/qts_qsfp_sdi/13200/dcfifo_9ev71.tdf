	aclr_inject0a[].d = B"1";
	aclr_inject0a[].ena = B"1";
	aclr_inject1a[].clk = rdclk;
	aclr_inject1a[].d = aclr_inject0a[].q;
	aclr_inject1a[].ena = B"1";
